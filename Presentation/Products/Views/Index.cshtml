@model IEnumerable<CleanArchitecture.Application.Products.Queries.GetProductsList.ProductListItemDto>

@{
    ViewBag.Title = "Products";
}

<h2>Products</h2>
<table class="table table-striped">
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var products in Model)
        {
            <tr>
                <td>@products.Id</td>
                <td>@products.Name</td>
            </tr>
        }
    </tbody>
</table>